{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "@sodiumlabs/plume-api",
    "version": "1.7.0",
    "description": "The official Plume API wrapper",
    "license": "MIT",
    "author": "SodiumLabs",
    "scripts": {
        "build": "tsc --noEmit && tsup",
        "lint": "tsc --noEmit && oxlint && prettier . --check",
        "format": "prettier . --write",
        "test": "vitest"
    },
    "exports": {
        "require": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "import": {
            "types": "./dist/index.d.mts",
            "default": "./dist/index.mjs"
        }
    },
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "keywords": [
        "api",
        "discord",
        "bot",
        "plume",
        "wrapper",
        "plumeapi",
        "plume-api",
        "sodium",
        "sodiumlabs"
    ],
    "homepage": "https://github.com/sodium-labs/plume-api.js#readme",
    "bugs": {
        "url": "https://github.com/sodium-labs/plume-api.js/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sodium-labs/plume-api.js.git"
    },
    "devDependencies": {
        "@types/node": "^22.18.6",
        "oxlint": "^1.16.0",
        "prettier": "^3.5.2",
        "tsup": "^8.5.0",
        "typescript": "^5.9.2",
        "vitest": "^3.2.4"
    }
}
